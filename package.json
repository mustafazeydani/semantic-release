{
  "name": "@mustafazeydani/semantic-release",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.0.0",
  "description": "Semantic release configuration and workflows for my personal projects",
  "author": "Mustafa Zeydani",
  "license": "MIT",
  "homepage": "https://github.com/mustafazeydani/semantic-release/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mustafazeydani/semantic-release.git"
  },
  "bugs": {
    "url": "https://github.com/mustafazeydani/semantic-release/issues"
  },
  "keywords": [
    "semantic-release"
  ],
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "bin": {
    "semantic-release": "bin/cli.js"
  },
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "format": "prettier --write src",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@commitlint/cli": "19.x",
    "@commitlint/config-conventional": "19.x",
    "husky": "9.x"
  },
  "peerDependenciesMeta": {
    "@commitlint/cli": {
      "optional": true
    },
    "@commitlint/config-conventional": {
      "optional": true
    },
    "husky": {
      "optional": true
    }
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "semantic-release": "^24.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2"
  },
  "pnpm": {
    "overrides": {
      "conventional-changelog-conventionalcommits": ">= 8.0.0"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
